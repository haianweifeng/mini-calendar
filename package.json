{
  "name": "mini-calendar",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint src/ --ext .ts,.tsx --no-error-on-unmatched-pattern",
    "lint:fix": "eslint src/ --fix --ext .ts,.tsx --no-error-on-unmatched-pattern"
  },
  "dependencies": {
